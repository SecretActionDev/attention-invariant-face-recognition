est_score\n"],"metadata":{"id":"DCorU6eTvofA","executionInfo":{"status":"ok","timestamp":1764262856326,"user_tz":300,"elapsed":10,"user":{"displayName":"Elmira Onagh","userId":"08687294645838767092"}}},"execution_count":23,"outputs":[]},{"cell_type":"code","source":["def process_wild_set(wild_img_dir, gallery, query_labels, open_set_threshold):\n","    rows = []\n","    img_paths = sorted(Path(wild_img_dir).glob(\"*.jpg\"))\n","\n","    for img_path in tqdm(img_paths, desc=\"Processing wild set\"):\n","        fname = img_path.name\n","        true_id = int(query_labels[fname])  # 0-6 for gallery, 7 = no match\n","\n","        img = load_image(img_path)\n","        emb = get_embedding(img)\n","\n","        if emb is None:\n","            pred_id = 7\n","            score = 0.0\n","            correct = 1 if true_id == 7 else 0\n","        else:\n","            pred_id, score = identify_face(emb, gallery, open_set_threshold=open_set_threshold)\n","            correct = 1 if pred_id == true_id else 0\n","\n","        rows.append({\n","            \"filename\": fname,\n","            \"true_id\": true_id,\n","            \"pred_id\": pred_id,\n","            \"score\": score,\n","            \"correct\": correct\n","        })\n","    return pd.DataFrame(rows)"],"metadata":{"id":"GPnYcUSJvhf7","executionInfo":{"status":"ok","timestamp":1764262858276,"user_tz":300,"elapsed":14,"user":{"displayName":"Elmira Onagh","userId":"08687294645838767092"}}},"execution_count":24,"outputs":[]},{"cell_type":"code","source":["print(\"=== Processing Wild Set ===\")\n","ground_truth_df = pd.read_csv(GROUND_TRUTH_CSV)\n","query_labels = dict(zip(ground_truth_df['filename'], ground_truth_df['True_ID']))\n","results = process_wild_set(WILD_IMG_DIR, gallery, query_labels, open_set_threshold=0.95)\n","results.to_csv(save_path + \"baseline_results.csv\", index=False)\n","print(\"Saved baseline_results.csv\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"pmAW_Ye-vORX","ex